                  "operating_countries": "xsd:string",
                  "industry_code": "xsd:string",
                  "payroll_contact_email": "xsd:string",
                  "payroll_contact_phone": "xsd:string",
                  "default_currency": "xsd:string",
                  "default_timezone": "xsd:string",
                  "payroll_software": "xsd:string",
                  "employee_count": "xsd:integer"
                }
              },
              "Worker": {
                "properties": {
                  "worker_id": "xsd:string",
                  "person_identifier": "xsd:string",
                  "first_name": "xsd:string",
                  "last_name": "xsd:string",
