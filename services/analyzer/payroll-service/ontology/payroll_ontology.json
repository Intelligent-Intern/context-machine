                  "start_date": "xsd:date",
                  "end_date": "xsd:date",
                  "probation_end_date": "xsd:date",
                  "base_currency": "xsd:string",
                  "base_pay_frequency": "xsd:string",
                  "base_pay_amount": "xsd:decimal",
