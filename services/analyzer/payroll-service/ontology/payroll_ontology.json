      "prov": "http://www.w3.org/ns/prov#",
      "time": "http://www.w3.org/2006/time#",
      "geo": "http://www.opengis.net/ont/geosparql#",
      "unit": "http://qudt.org/vocab/unit#",
      "gr": "http://purl.org/goodrelations/v1#",
      "acc": "http://context-machine.org/ontology/accounting#",
      "tax": "http://context-machine.org/ontology/tax#",
      "benefit": "http://context-machine.org/ontology/benefit#",
      "bank": "http://context-machine.org/ontology/banking#",
