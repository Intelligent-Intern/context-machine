                  "city": "xsd:string",
                  "postal_code": "xsd:string",
                  "geo_lat": "xsd:decimal",
                  "geo_lon": "xsd:decimal",
                  "timezone": "xsd:string",
                  "jurisdiction_id": "xsd:string"
                }
              }
            }
          }
        }
      },

