                  "vessel_id": "xsd:string",
                  "type": "xsd:string",
                  "diameter_m": "xsd:float",
                  "pitch_m": "xsd:float",
                  "blades": "xsd:integer",
                  "cp_control_system": "xsd:string"
                }
              },
              "Gearbox": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "ratio": "xsd:string",
                  "input_rpm": "xsd:float",
                  "output_rpm": "xsd:float",
                  "lubrication_type": "xsd:string"
                }
              },
              "Thruster": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "vessel_id": "xsd:string",
                  "thruster_type": "xsd:string",
                  "location": "xsd:string",
                  "rated_thrust_kn": "xsd:float",
                  "power_kw": "xsd:float"
                }
              },
              "SteeringGear": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "type": "xsd:string",
                  "torque_knm": "xsd:float",
                  "power_unit_count": "xsd:integer"
                }
              }
            }
          },
          "Auxiliary_And_Utility_Systems": {
            "entities": {
              "AuxEngineGenerator": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "vessel_id": "xsd:string",
                  "tag_number": "xsd:string",
                  "make": "xsd:string",
                  "model": "xsd:string",
                  "prime_mover_type": "xsd:string",
                  "mcr_kw": "xsd:float",
                  "alternator_kva": "xsd:float",
                  "voltage_v": "xsd:float",
                  "frequency_hz": "xsd:float",
                  "running_hours_total": "xsd:float",
                  "last_service_date": "xsd:date"
                }
              },
              "Boiler": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "type": "xsd:string",
                  "steam_capacity_t_per_h": "xsd:float",
                  "pressure_bar": "xsd:float",
                  "fuel_type": "xsd:string"
                }
              },
              "Pump": {
                "properties": {
                  "equipment_id": "xsd:string",
