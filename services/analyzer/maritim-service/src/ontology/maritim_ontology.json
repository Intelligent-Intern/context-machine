                  "model": "xsd:string",
                  "serial_number": "xsd:string",
                  "engine_type": "xsd:string",
                  "cylinders": "xsd:integer",
                  "bore_mm": "xsd:float",
                  "stroke_mm": "xsd:float",
                  "mcr_kw": "xsd:float",
                  "sfc_g_per_kwh": "xsd:float",
                  "nox_tier": "xsd:string",
                  "egr_installed": "xsd:boolean",
                  "scr_installed": "xsd:boolean",
                  "shaft_power_limitation_kw": "xsd:float",
                  "rated_rpm": "xsd:float",
                  "fuel_types": "xsd:string",
                  "running_hours_total": "xsd:float",
                  "last_overhaul_date": "xsd:date",
                  "next_overhaul_due_hours": "xsd:float",
                  "location_compartment_id": "xsd:string"
                }
              },
              "Propeller": {
                "properties": {
