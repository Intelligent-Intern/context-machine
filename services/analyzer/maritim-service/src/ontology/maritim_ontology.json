                  "effective_date": "xsd:date"
                },
                "instances": {
                  "SOLAS": {
                    "name": "SOLAS",
                    "issuer": "IMO",
                    "scope": "Safety of Life at Sea"
                  },
                  "MARPOL_Annex_VI": {
                    "name": "MARPOL Annex VI",
                    "issuer": "IMO",
                    "scope": "Air pollution/NOx/SOx/CO2"
                  },
                  "ISM_Code": {
                    "name": "ISM Code",
                    "issuer": "IMO",
                    "scope": "Safety Management"
                  },
                  "MLC_2006": {
