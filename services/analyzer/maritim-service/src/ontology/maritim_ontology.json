                }
              }
            }
          },
          "Voyage_Data_Recorders": {
            "entities": {
              "VDRRecord": {
                "properties": {
                  "record_id": "xsd:string",
                  "vessel_id": "xsd:string",
                  "start_time": "xsd:dateTime",
                  "end_time": "xsd:dateTime",
                  "channels": "xsd:string",
                  "storage_uri": "xsd:string",
                  "checksum": "xsd:string"
                }
              }
            }
          }
        }
      },
      "Navigation_And_Communication_System": {
        "collections": {
          "Bridge_Systems": {
            "entities": {
