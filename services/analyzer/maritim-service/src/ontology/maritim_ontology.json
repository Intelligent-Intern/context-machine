{
  "ontology": {
    "name": "maritime_ontology",
    "version": "1.0.0",
    "namespace": "http://context-machine.org/ontology/maritime#",
    "description": "Comprehensive maritime ontology for fleet & vessel management, machinery, electrical & automation, navigation & GMDSS, cargo operations, maintenance/PMS, regulatory compliance (SOLAS/MARPOL/ISM/MLC/BWM, EU MRV/DCS/ETS), safety & security (LSA/FFE/ISPS), energy & emissions (EEXI/CII/SEEMP), port logistics, shipyard & modifications, documents, sensors/IIoT/VDR and federated access policies.",
    "namespaces": {
      "mar": "http://context-machine.org/ontology/maritime#",
      "ship": "http://context-machine.org/ontology/maritime/ship#",
      "sys": "http://context-machine.org/ontology/maritime/systems#",
      "mach": "http://context-machine.org/ontology/maritime/machinery#",
      "elec": "http://context-machine.org/ontology/maritime/electrical#",
      "auto": "http://context-machine.org/ontology/maritime/automation#",
      "nav": "http://context-machine.org/ontology/maritime/navigation#",
      "cargo": "http://context-machine.org/ontology/maritime/cargo#",
      "safety": "http://context-machine.org/ontology/maritime/safety#",
      "sec": "http://context-machine.org/ontology/maritime/security#",
      "crew": "http://context-machine.org/ontology/maritime/crew#",
      "reg": "http://context-machine.org/ontology/maritime/regulation#",
      "class": "http://context-machine.org/ontology/maritime/class#",
      "port": "http://context-machine.org/ontology/maritime/port#",
      "voy": "http://context-machine.org/ontology/maritime/voyage#",
      "maint": "http://context-machine.org/ontology/maritime/maintenance#",
      "spares": "http://context-machine.org/ontology/maritime/spares#",
      "doc": "http://context-machine.org/ontology/maritime/document#",
      "sensor": "http://context-machine.org/ontology/maritime/sensor#",
      "vdr": "http://context-machine.org/ontology/maritime/vdr#",
      "env": "http://context-machine.org/ontology/maritime/environment#",
      "energy": "http://context-machine.org/ontology/maritime/energy#",
      "fuel": "http://context-machine.org/ontology/maritime/fuel#",
      "risk": "http://context-machine.org/ontology/maritime/risk#",
      "cyber": "http://context-machine.org/ontology/maritime/cyber#",
      "contract": "http://context-machine.org/ontology/maritime/contract#",
      "geo": "http://www.w3.org/2003/01/geo/wgs84_pos#",
      "gsp": "http://www.opengis.net/ont/geosparql#",
      "time": "http://www.w3.org/2006/time#",
      "qudt": "http://qudt.org/vocab/unit#",
      "sosa": "http://www.w3.org/ns/sosa/",
      "ssn": "http://www.w3.org/ns/ssn/",
      "dc": "http://purl.org/dc/elements/1.1/",
      "foaf": "http://xmlns.com/foaf/0.1/",
      "prov": "http://www.w3.org/ns/prov#",
      "xsd": "http://www.w3.org/2001/XMLSchema#"
    },
    "imports": [
      "http://context-machine.org/ontology/universal#",
      "http://www.opengis.net/ont/geosparql#",
      "http://www.w3.org/2006/time#",
      "http://qudt.org/vocab/unit#",
      "http://purl.org/dc/elements/1.1/",
      "http://xmlns.com/foaf/0.1/",
      "http://www.w3.org/ns/prov#",
      "http://www.w3.org/2003/01/geo/wgs84_pos#",
      "http://www.w3.org/ns/sosa/",
      "http://www.w3.org/ns/ssn/"
    ],
    "frames": {
      "Fleet_And_Vessel_System": {
        "collections": {
          "Fleet_Registry": {
            "entities": {
              "Company": {
                "properties": {
                  "company_id": "xsd:string",
                  "name": "xsd:string",
                  "role": "xsd:string",
                  "imo_company_number": "xsd:string",
                  "country": "xsd:string",
                  "address": "xsd:string",
                  "contact_email": "xsd:string",
                  "contact_phone": "xsd:string"
                }
              },
              "Fleet": {
                "properties": {
                  "fleet_id": "xsd:string",
                  "fleet_name": "xsd:string",
                  "owner_company_id": "xsd:string",
                  "manager_company_id": "xsd:string",
                  "vessel_count": "xsd:integer"
                }
              }
            }
          },
          "Vessel_Basics": {
            "entities": {
              "Vessel": {
                "properties": {
                  "vessel_id": "xsd:string",
                  "imo_number": "xsd:string",
                  "mmsi": "xsd:string",
                  "call_sign": "xsd:string",
                  "vessel_name": "xsd:string",
                  "vessel_type": "xsd:string",
                  "sub_type": "xsd:string",
                  "flag_state": "xsd:string",
                  "registry_port": "xsd:string",
                  "class_society": "xsd:string",
                  "class_notation": "xsd:string",
                  "ice_class": "xsd:string",
                  "dp_class": "xsd:string",
                  "year_built": "xsd:integer",
                  "builder": "xsd:string",
                  "hull_number": "xsd:string",
                  "status": "xsd:string",
                  "owner_company_id": "xsd:string",
                  "manager_company_id": "xsd:string",
                  "operator_company_id": "xsd:string",
                  "doc_company_id": "xsd:string",
                  "gross_tonnage": "xsd:float",
                  "net_tonnage": "xsd:float",
                  "deadweight_tonnes": "xsd:float",
                  "length_overall_m": "xsd:float",
                  "length_bp_m": "xsd:float",
                  "breadth_molded_m": "xsd:float",
                  "depth_m": "xsd:float",
                  "design_draft_m": "xsd:float",
                  "summer_draft_m": "xsd:float",
                  "service_speed_kn": "xsd:float",
                  "economic_speed_kn": "xsd:float",
                  "crew_complement": "xsd:integer",
                  "primary_fuel": "xsd:string",
                  "alternative_fuels": "xsd:string",
                  "propulsion_type": "xsd:string",
                  "eexi_g_co2_per_tnm": "xsd:float",
                  "cii_rating_last_year": "xsd:string",
                  "position": "geo:Point",
                  "last_ais_update": "xsd:dateTime",
                  "last_drydock_date": "xsd:date",
                  "digital_twin_id": "xsd:string"
                }
              },
              "Compartment": {
                "properties": {
                  "compartment_id": "xsd:string",
                  "vessel_id": "xsd:string",
                  "name": "xsd:string",
                  "type": "xsd:string",
                  "deck": "xsd:string",
                  "frame_start": "xsd:string",
                  "frame_end": "xsd:string",
                  "capacity_m3": "xsd:float",
                  "service_medium": "xsd:string",
                  "hazardous_zone": "xsd:string",
                  "inerted": "xsd:boolean",
                  "location_polygon": "gsp:Polygon"
                }
              }
            }
          },
          "Vessel_Structure": {
            "entities": {
              "HullSegment": {
                "properties": {
                  "segment_id": "xsd:string",
                  "vessel_id": "xsd:string",
                  "location": "xsd:string",
                  "material": "xsd:string",
                  "coating_system": "xsd:string",
                  "thickness_mm": "xsd:float",
                  "corrosion_allowance_mm": "xsd:float",
                  "last_utm_date": "xsd:date"
                }
              }
            }
          }
        }
      },
      "Propulsion_And_Machinery_System": {
        "collections": {
          "Propulsion": {
            "entities": {
              "MainEngine": {
                "properties": {
