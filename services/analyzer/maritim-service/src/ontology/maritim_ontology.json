                  "head_m": "xsd:float",
                  "driver_type": "xsd:string"
                }
              },
              "HeatExchanger": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "type": "xsd:string",
                  "duty_kw": "xsd:float",
                  "primary_medium": "xsd:string",
                  "secondary_medium": "xsd:string"
                }
              },
              "BWTSystem": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "type": "xsd:string",
                  "maker": "xsd:string",
                  "capacity_m3_h": "xsd:float",
                  "imo_type_approval": "xsd:string",
                  "uscg_approved": "xsd:boolean"
                }
              },
              "OilyWaterSeparator": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "capacity_m3_h": "xsd:float",
                  "ppm_alarm_type": "xsd:string"
                }
              },
              "SewageTreatmentPlant": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "capacity_persons": "xsd:integer",
                  "standard": "xsd:string"
                }
              },
              "FreshWaterGenerator": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "type": "xsd:string",
                  "capacity_m3_d": "xsd:float"
                }
              }
            }
          }
        }
      },
      "Electrical_Power_System": {
        "collections": {
          "Power_Distribution": {
            "entities": {
              "Switchboard": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "voltage_v": "xsd:float",
                  "short_circuit_rating_ka": "xsd:float",
                  "sections": "xsd:integer",
                  "bus_tie_breakers": "xsd:integer",
                  "protection_scheme": "xsd:string"
                }
              },
              "Transformer": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "rating_kva": "xsd:float",
                  "primary_v": "xsd:float",
                  "secondary_v": "xsd:float"
                }
              },
              "FrequencyConverter": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "input_v": "xsd:float",
                  "output_v": "xsd:float",
                  "output_hz": "xsd:float",
                  "power_kw": "xsd:float"
                }
              },
              "BatterySystem": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "chemistry": "xsd:string",
                  "capacity_kwh": "xsd:float",
                  "max_discharge_kw": "xsd:float",
                  "soh_percentage": "xsd:float"
                }
              },
              "ShorePowerInterface": {
                "properties": {
                  "equipment_id": "xsd:string",
                  "standard": "xsd:string",
                  "max_power_mva": "xsd:float",
                  "voltage_class": "xsd:string"
                }
              }
            }
          }
        }
      },
      "Automation_Monitoring_Control_System": {
        "collections": {
